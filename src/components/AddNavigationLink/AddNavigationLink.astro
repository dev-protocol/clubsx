---
import type {
  ClubsConfiguration,
  ClubsPropsAdminPages,
} from '@devprotocol/clubs-core'
import AddNavigationLink_ from './AddNavigationLink.svelte'
import type { NavLink } from '@constants/navLink'

const { forAddNavigationLink } = Astro.props as ClubsPropsAdminPages & {
  forAddNavigationLink: {
    config: ClubsConfiguration
    label: string
    link: NavLink
  }
}
---

<AddNavigationLink_
  client:load
  config={forAddNavigationLink.config}
  label={forAddNavigationLink.label}
  link={forAddNavigationLink.link}
/>
