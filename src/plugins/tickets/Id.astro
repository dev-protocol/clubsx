---
import type { Membership } from '@plugins/memberships'
import type { Ticket } from '.'
import Id from './Id.svelte'

const { ticket, membership, propertyAddress, rpcUrl, ...other } =
  Astro.props as {
    ticket: Ticket
    membership: Membership
    propertyAddress: string
    rpcUrl: string
  }
---

<Id client:load ticket={ticket} membership={membership} />
