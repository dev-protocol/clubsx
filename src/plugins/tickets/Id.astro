---
import type { Membership } from '@plugins/memberships'
import type { Ticket } from '.'
import Id_ from './Id.svelte'
import type { BanningRules } from './utils/get-banning-rules'

type Props = {
  ticket: Ticket
  membership?: Membership
  erc721Enumerable?: string
  rpcUrl: string
  ban: BanningRules
}

const { ticket, membership, erc721Enumerable, rpcUrl, ban } = Astro.props

const tokenId = Number(Astro.url.searchParams.get('id'))
---

<Id_
  client:load
  ticket={ticket}
  membership={membership}
  erc721Enumerable={erc721Enumerable}
  tokenId={tokenId}
  rpcUrl={rpcUrl}
  ban={ban}
/>
