---
import JoinCheckout from '@components/Join/JoinCheckout.vue'

const {
  amount,
  propertyAddress,
  currency,
  rpcUrl,
  feeBeneficiary,
  feePercentage,
} = Astro.props
---

<JoinCheckout
  client:only="vue"
  amount={amount}
  destination={propertyAddress}
  currency={currency}
  feeBeneficiary={feeBeneficiary}
  feePercentage={feePercentage}
  page="JOIN"
  rpcUrl={rpcUrl}
/>
