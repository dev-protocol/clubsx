---
import { decode } from '@devprotocol/clubs-core'
import AdminPlugins from '@components/AdminPlugins/AdminPlugins.jsx'

const { clubs } = Astro.props
const { plugins } = decode(clubs.encodedClubsConfiguration)
const enabledPlugins = plugins.filter(
  (plugin) => plugin.name.toUpperCase() !== 'ADMIN'
)
---

<AdminPlugins plugins={enabledPlugins} />
