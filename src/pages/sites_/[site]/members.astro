---
import Layout from '@layouts/Default.astro'

import Supporters from '@components/Members/Supporters.vue'
import Sidebar from '@components/Sidebar/Sidebar.vue'
---

<Layout metaTitle="Dev Protocol | An open protocol for Creator Economy">
  <div
    class="lg:container relative mx-auto lg:pt-12 grid lg:gap-12 lg:grid-cols-[minmax(290px,_1fr)_4fr]"
  >
    <Sidebar client:load />
    <div class="mx-4 mb-4">
      <h2 class="font-title font-bold text-3xl">Members</h2>
      <Supporters client:only="vue" />
    </div>
  </div>
</Layout>
